<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white"/>
            <corners android:radius="@dimen/round_radius_small"/>
        </shape>
    </item>
    <item android:left="@dimen/contour_offset" android:top="@dimen/contour_offset" android:right="@dimen/contour_offset" android:bottom="@dimen/contour_offset">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/split_line_width" android:color="@color/split_line_color"/>
            <corners android:radius="@dimen/round_radius_small"/>
        </shape>
    </item>
</layer-list>
