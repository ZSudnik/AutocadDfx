<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/text_color">
    <item android:id="@android:id/mask">
        <shape android:padding="2dp" android:shape="rectangle">
            <solid android:color="@color/text_color"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item>
        <shape android:padding="2dp" android:shape="rectangle">
            <stroke android:width="2px" android:color="@color/text_color" android:dashWidth="4px" android:dashGap="4px"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
</ripple>
